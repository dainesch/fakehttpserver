<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="borderPane" prefHeight="349.0" prefWidth="768.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lu.dainesch.fakehttp.view.settings.SettingsPresenter">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <left>
      <ListView fx:id="settingsList" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets right="10.0" />
         </BorderPane.margin>
      </ListView>
   </left>
   <center>
      <VBox prefHeight="345.0" prefWidth="572.0" BorderPane.alignment="CENTER">
         <children>
            <GridPane nodeOrientation="LEFT_TO_RIGHT">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="Server Port :" GridPane.rowIndex="1" />
                  <TextField fx:id="serverPortLabel" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <TextField fx:id="previewSizeLabel" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <Label text="Request Preview Size : " GridPane.rowIndex="2" />
               </children>
            </GridPane>
         </children>
      </VBox>
   </center>
</BorderPane>
